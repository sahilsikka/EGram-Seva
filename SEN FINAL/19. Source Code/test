DROP TRIGGER IF EXISTS `_after_ins_tr2`;
DELIMITER //
CREATE TRIGGER `_after_ins_tr2` AFTER INSERT ON `useraccount`
 FOR EACH ROW BEGIN

declare str VARCHAR(100);
declare str1 VARCHAR(100);
declare mob VARCHAR(20);

set str=(select concat('password: ',password) from useraccount 
					order by reg_mod_date desc limit 1);

set mob=(select mobile from useraccount order by reg_mod_date desc limit 1);

insert into `messageout`(MessageTo,MessageFrom,MessageText) VALUES (mob,"+919624122862",str);

END
//
DELIMITER ;


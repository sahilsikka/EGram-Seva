--
-- Triggers `messagein`
--
DROP TRIGGER IF EXISTS `_after_ins_tr3`;
DELIMITER //
CREATE TRIGGER `_after_ins_tr3` AFTER INSERT ON `messagein`
 FOR EACH ROW BEGIN
DECLARE Count INT;
DECLARE mob VARCHAR(200);
DECLARE date VARCHAR(200);
DECLARE mob_select BIGINT;
DECLARE IntNumbers VARCHAR(200);
DECLARE Strin VARCHAR(1000);
DECLARE Strin_h VARCHAR(5000);
DECLARE tim_lat TIMESTAMP;
DECLARE temp_pass VARCHAR(20);
DECLARE reg_check VARCHAR(20);
declare str_health varchar(5000);
declare ro int;
declare i int;
Declare sub varchar(5); 
SET Count = 1;
SET IntNumbers = '';

SET GLOBAL event_scheduler = ON;

set Strin = (select MessageText from `Messagein` order by receiveTime desc limit 1);
set Strin_h = (select MessageText from `messagein` order by receiveTime desc limit 1);
set mob = (select MessageFrom from `messagein` order by receiveTime desc limit 1);
set mob_select=(select count(*) from (select mobile from `useraccount` where mobile=mob limit 1)as r);
set date=(select receiveTime from `messagein` order BY receiveTime desc limit 1);

set ro=(select COUNT(*) from temp_main_db where mobile=mob and reg_code=3);

if((strin_h='1' or strin_h='2' or strin_h='3' or strin_h='4' or strin_h='5'

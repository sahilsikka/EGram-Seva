     VALUES (mob,"+919624122862","गलत संदेश. REG 1 2 3 भेजे या स्वास्थ्य संबंधित प्रश्नों के लिए संख्या 1-15 के बीच की भेजें");
     
 END if;

ELSE 
set reg_check=SUBSTRING(strin,1,3);
if(reg_check='reg' or reg_check='REG' or reg_check='Reg')
THEN
		SET IntNumbers =SUBSTRING(Strin,4,6);

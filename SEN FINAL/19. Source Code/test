		AND SUBSTRING(Strin,Count,1) <= '9') then

		SET IntNumbers = SUBSTRING(Strin,Count,1);
		insert into `temp_main_db` VALUES(IntNumbers,mob);
	END IF;
	SET Count = Count + 1;


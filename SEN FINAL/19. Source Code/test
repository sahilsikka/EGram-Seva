--
-- Triggers `weather`
--
DROP TRIGGER IF EXISTS `_after_upd_tr3`;
DELIMITER //
CREATE TRIGGER `_after_upd_tr3` AFTER UPDATE ON `weather`
 FOR EACH ROW BEGIN

update crops 
set name=(select concat(info_date,'-',description) as q from weather) where reg_code=2;

END
//
DELIMITER ;

DELIMITER $$
--
-- Events
--
CREATE DEFINER=`root`@`localhost` EVENT `event` ON SCHEDULE EVERY 1 YEAR STARTS '2013-04-08 17:36:00' ON COMPLETION PRESERVE ENABLE DO BEGIN

DECLARE i INT;
DECLARE coun int;
DECLARE mob VARCHAR(1000);
DECLARE msg VARCHAR(1000);

set i=0;
SET coun=(select count(*)from (select v.mobile,group_concat(v.name separator ', ') as rt from 
(select mobile,name from temp_main_db join crops on `crops`.reg_code=`temp_main_db`.reg_code)
 as v group by mobile) as r);

while i<coun
do
set mob=	
(select v.mobile from 
(select mobile,name from temp_main_db join crops on `crops`.reg_code=`temp_main_db`.reg_code)
 as v group by mobile limit 1 offset i);  
SET NAMES UTF8;
set msg=
(select group_concat(v.name separator ', ') as rt from 
(select mobile,name from temp_main_db join crops on `crops`.reg_code=`temp_main_db`.reg_code)
 as v group by mobile limit 1 offset i);  

INSERT INTO `messageout`(`MessageTo`, `MessageFrom`, `MessageText`)
 VALUES (mob,"+919624122862",msg);
 
set i=i+1;
END while;
END$$

DELIMITER ;


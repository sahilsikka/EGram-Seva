--
-- Triggers `ex`
--
DROP TRIGGER IF EXISTS `_after_ins_tr1`;
DELIMITER //
CREATE TRIGGER `_after_ins_tr1` AFTER INSERT ON `ex`
 FOR EACH ROW begin
declare count int;
set count=(select count(*) from `temp_crops`);

If(COUNT!=0)
	then

		update `temp_crops` 
		set conc_text=(select group_concat(v.q separator ', ') as rt from 
					(select concat(seeds,'-',price,New) as q from ex where (seeds='Onion' or seeds='Potato'
					or seeds='Green pepper' or seeds='Eggplant' or seeds='Cotton' or seeds='Arnde Seed'
					or seeds='Wheat' or seeds='Barley')and (price!=0 and New!=0 and new1!=0))
					as v group by 'all');

	else

		insert into `temp_crops`
		VALUES(1,(select group_concat(v.q separator ', ') as rt from 
		(select concat(seeds,'-',price,New) as q from ex where (seeds='Onion' or seeds='Potato'
		or seeds='Green pepper' or seeds='Eggplant' or seeds='Cotton' or seeds='Arnde Seed'
		or seeds='Wheat' or seeds='Barley')and (price!=0 or New!=0 or new1!=0))
		as v group by 'all'));

END IF;
end
//

DELIMITER ;
DROP TRIGGER IF EXISTS `_after_upd_tr1`;
DELIMITER //
CREATE TRIGGER `_after_upd_tr1` AFTER UPDATE ON `ex`
 FOR EACH ROW begin
declare count int;
set count=(select count(*) from `temp_crops`);

If(COUNT!=0)
then

	update `temp_crops` 
	set conc_text=(select group_concat(v.q separator ', ') as rt from 
	(select concat(seeds,'-',price,New,new1) as q from ex where (seeds='Onion' or seeds='Potato'
	or seeds='Green pepper' or seeds='Eggplant' or seeds='Cotton' or seeds='Arnde Seed'
	or seeds='Wheat' or seeds='Barley') and (price!=0 or New!=0 OR new1!=0))
	as v group by 'all');

else

	insert into `temp_crops`
	VALUES(1,(select group_concat(v.q separator ', ') as rt from 
	(select concat(seeds,'-',price,New) as q from ex where (seeds='Onion' or seeds='Potato'
	or seeds='Green pepper' or seeds='Eggplant' or seeds='Cotton' or seeds='Arnde Seed'
	or seeds='Wheat' or seeds='Barley')and (price!=0 or New!=0 or new1!=0))
	as v group by 'all'));

END IF;
end
//
DELIMITER ;

-- --------------------------------------------------------

--
-- Table structure for table `messagein`
--

CREATE TABLE IF NOT EXISTS `messagein` (
  `Id` int(11) NOT NULL AUTO_INCREMENT,
  `SendTime` datetime DEFAULT NULL,
  `ReceiveTime` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `MessageFrom` varchar(80) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `MessageTo` varchar(80) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `SMSC` varchar(80) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `MessageText` varchar(1000) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `MessageType` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `MessagePDU` text CHARACTER SET utf8 COLLATE utf8_unicode_ci,
  `Gateway` varchar(80) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `UserId` varchar(80) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  PRIMARY KEY (`Id`)
) ENGINE=InnoDB  DEFAULT CHARSET=utf8 AUTO_INCREMENT=185 ;

